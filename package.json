{
  "name": "qrshared",
  "displayName": "QRshared",
  "description": "Share code by QR Code",
  "version": "0.0.4",
  "publisher": "QRshared",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:qrshared.shareCode",
    "onCommand:qrshared.receiveCode"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "qrshared.shareCode",
        "title": "QRshared: Share Code by QR Code"
      },
      {
        "command": "qrshared.receiveCode",
        "title": "QRshared: Receive Code via QR Code"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/qrcode": "^1.5.0",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "imgur": "^2.2.0",
    "jimp": "^0.16.0",
    "jsqr": "^1.4.0",
    "node-lzw": "^0.3.1",
    "qrcode": "^1.5.1",
    "tslib": "^2.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ThisisYoYoDev/QRshared"
  }
}
